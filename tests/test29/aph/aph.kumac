  ve/copy $sigma(0.2*avrg/avrg) dxavrg
  h/errors xavrg $sigma(avrg+savrg) dxavrg ? [npt] 20  .002
  h/errors xavrg $sigma(avrg-savrg) dxavrg ? [npt] 20  .002
  exec ~/PTOOL/black
  sum  = [sump]
  dsum = $sigma(sqrt([sums]))

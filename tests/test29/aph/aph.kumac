set mtyp 24
exec ~/PTOOL/legend  0.32 0.92 ' ' 0. 0.35 40 0
exec ~/PTOOL/snotes  0.27 0.92 'e^+!e^-!' 0. 0.35 40 0
set mtyp
exec ~/PTOOL/legline 0.54 0.92 'CHIPS MC' 0. 0.35 40 0
set dmod

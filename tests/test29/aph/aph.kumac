macro aph f=1 gen=1 com=0 scs=0.93 scm=1.10
*
  run=2000*[f]
  hi/del *
  ve/del *
  pi/del *
  exec creavec
  clo 35
  targ = H
  file = h0100
  if [run] > 0 then
    exec chips [targ] [file] 1 0 [run]
  endif
  exec readpar pf=chipstest_[file].in
*
  opt zfl
  swi gz
*
  if [gen] = 1 then
    opt nsta
    clr
    exec MomMul [scs] [scm]
*@@*    exec Channels
*QQ*    exec ThreeChn
*QQ*    exec TwoChn
  endif
  if [com] = 1 then
	   opt htit
    opt stat
    exec 3pi0
    exec pipm0
    exec 3K
    exec pieep
  endif
*
return

  graph [nb] $sigma(0.5*(wp+wt)) $sigma(wy/wp) l
  set lwid 1
  set dmod 1
  h/errors $sigma(0.5*(wp+wt)) $sigma(wy/wp) ? $sigma(wd/wp) [nb] 1 .01
  ve/del wx,wy,wd,wt,wp

#
# Macro file test06.in
# for tests of basic e.m. processes in integral approach + new p.e. effect
#   (G4PhotoAbsorption).
#
/control/verbose 2
# /run/verbose 2
/run/initialize
/gun/direction 1 0 0
#
# gamma 500 MeV in Al
#
/mydet/SelectMaterial Al
/gun/particle gamma
/gun/energy 500 MeV
/run/beamOn 100
#
# electron 500 MeV in Air
#
/mydet/SelectMaterial Air
/gun/particle e-
/gun/energy 500 MeV
/run/beamOn 100
#
exit

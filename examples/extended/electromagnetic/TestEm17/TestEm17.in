#
# Macro file for "TestEm17.cc"
# (can be run in batch, without graphic)
#
/control/verbose 2
/run/verbose 2
#
/testem/det/setMat Iron
/testem/det/setSize 1 m
#
/testem/phys/addPhysics emstandard_opt0
/run/setCut 1 mm
#
/run/initialize
#
/gun/particle mu+
/gun/energy 10 TeV
#
/run/printProgress 1000
#
/run/beamOn 10000
#
/gun/particle pi+
/run/beamOn 10000
#
/gun/particle proton
/run/beamOn 10000
#

